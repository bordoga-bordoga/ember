{
  "name": "emberbot",
  "version": "0.1.0",
  "description": "TypeScript functional discord.js bot utilizing akairo as a framework, with a Prisma->Nexus->Apollo->GraphQL API",
  "main": "dist/index.js",
  "keywords": [
    "discord"
  ],
  "author": "Bryn",
  "license": "MIT",
  "private": "false",
  "engines": {
    "node": ">=15.3.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@prisma/client": "^2.13.1",
    "apollo-server": "^2.19.0",
    "common-tags": "^1.8.0",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "fuzzy-search": "^3.2.1",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.27.0",
    "react": "^17.0.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.12.24",
    "@microsoft/tsdoc-config": "^0.13.9",
    "@prisma/cli": "^2.13.1",
    "@types/common-tags": "^1.8.0",
    "@types/fuzzy-search": "^2.1.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/pg": "^7.14.7",
    "@types/react": "^17.0.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^10.0.2",
    "eslint-config-marine": "^7.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "eslint-plugin-typescript-sort-keys": "^1.5.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "jest": "^26.6.3",
    "jest-discordjs-mocks": "^1.0.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typedoc": "^0.19.2",
    "typedoc-neo-theme": "^1.0.10",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "node .",
    "clean": "rimraf dist",
    "build": "tsc",
    "ndev": "nodemon -e ts --exec \"npm chain\"",
    "dev": "NODE_ENV=development ts-node-dev --transpile-only --no-notify src/index.ts",
    "stage": "prisma migrate dev --preview-feature --name staging",
    "stagedit": "code-insiders -r prisma/migrations/`npx prisma migrate dev --create-only --name stage --preview-feature | grep -o '\\b\\w*\\_stage\\b' | tail -1`/migration.sql",
    "migrate": "prisma migrate dev --preview-feature --name staging && prisma migrate deploy --preview-feature",
    "nexusdev": "NODE_ENV=development ts-node-dev --transpile-only --no-notify src/util/api/index.ts",
    "chain": "tsc && node .",
    "cleanchain": "rimraf dist && tsc && node .",
    "fullchain": "rimraf dist && tsc && typedoc && node .",
    "nexusgenerate": "ts-node --transpile-only src/util/api/schema/index.ts",
    "nexustest": "npm run generate && jest",
    "lint": "eslint . --ext .ts",
    "lintfix": "eslint . --ext .ts --fix"
  }
}
